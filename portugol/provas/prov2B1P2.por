programa {
  funcao inicio() {
    real conta , contPpessoa
    inteiro pessoa
    escreva (" Qual é a quantidade de pessoas ? : ")
    leia (pessoa)
    escreva(" Qual foi o valor da conta ? : ")
    leia(conta)
    contPpessoa = conta / pessoa
    escreva (" O valor que cada pessoa deve pagar é " , contPpessoa)
  }
}
